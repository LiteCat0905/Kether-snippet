{
    "comments": {
        // 用于单行注释的符号。如果您的语言不支持行注释，请删除此条目
        "lineComment": "#",
        // 用于开始和结束块注释的符号。如果您的语言不支持块注释，请删除此条目
        // "blockComment": [ "/*", "*/" ]
    },
    // 用作括号的符号
    "brackets": [
        ["{ ", " }"],
        ["[", "]"],
        ["(", ")"]
    ],
    // 键入时自动关闭的符号
    "autoClosingPairs": [
        ["{ ", " }"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        { "open": "'", "close": "'", "notIn": ["string", "comment"] },
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
    ],
    // 可用于环绕选定内容的符号
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ]
}